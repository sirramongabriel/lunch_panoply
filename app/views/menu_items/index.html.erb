<h1>MenuItems#index</h1>
<p>Find me in app/views/menu_items/index.html.erb</p>
<%#= @menu_items.inspect %>
<ul>
<% @menu_items.each do |dish| %>
  <!-- <li><%#= dish[:full_image_path] %></li> -->
  <li><strong>Entree</strong>: <%= dish[:name] %></li>
  <li><strong>Course</strong>: <%= dish[:course] %></li>
  <li><strong>Description</strong>: <%= dish[:description] %></li>
  <li><strong>Price</strong>: $<%#= sprintf('%.2f', dish[:price_cents]) %></li><hr />
<% end %>
</ul>
